<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script
      src="https://kit.fontawesome.com/64d58efce2.js"
      crossorigin="anonymous"
    ></script>
    <link rel="stylesheet" href="styles/styles_l.css" />
    <title>Tienda</title>
  </head>
  <body>
    <div class="container">
      <div class="forms-container">
        <div class="signin-signup">



          <form action="/verifyPedido" method="post" class="sign-in-form">
            <h2 class="title">Hacer Pedido</h2>
            <div class="input-field">
              <i class="fas fa-user"></i>
              <input id="Ciudad" name="Ciudad" type="text" placeholder="Ciudad" />
            </div>
            <div class="input-field">
              <i class="fas fa-lock"></i>
              <input id="Direccion" name="Direccion" type="text" placeholder=Direccion" />
            </div>
            <input type="submit" value="Ingresar" class="btn solid" />
          </form>


        
    <script src="js/app.js"></script>
  </body>
</html>
